                    Y.one('#bugfilters-portlet-content').set(
                        'innerHTML', response.responseText);
                },
                failure: function() {
                    Y.one('#bugfilters-portlet-content').set('innerHTML', '');
                }
            };
            var request = Y.io(url, {on: handlers});
        });
